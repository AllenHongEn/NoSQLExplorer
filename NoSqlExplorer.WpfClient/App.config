<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="dockerConfig" type="NoSqlExplorer.DockerAdapter.ConfigSection.DockerConfigSection, NoSqlExplorer.DockerAdapter"/>
    <section name="azureConfig" type="NoSqlExplorer.AzureAdapter.Configuration.AzureConfigSection, NoSqlExplorer.AzureAdapter"/>
  </configSections>

  <appSettings file="Secrets.config">
    <add key="twitter:feed-url" value="https://stream.twitter.com/1.1/statuses/sample.json"/>
  </appSettings>

  <dockerConfig configSource="Docker.config"/>

  <azureConfig configSource="Azure.config"/>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
</configuration>